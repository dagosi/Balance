<%= simple_form_for(@register) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :references %>
    <%= f.input :date %>
    <%= f.input :description %>
    <%= f.input :amount %>
    <%= f.input :type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
